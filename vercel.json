{
    "version": 2,
    "builds": [
        {
            "src": "index.php",
            "use": "vercel-php@0.6.0"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.php"
        }
    ],
    "env": {
        "PHP_VERSION": "8.1",
        "OPENSSL_CONF": "/usr/local/openssl/openssl.cnf"
    }
}
