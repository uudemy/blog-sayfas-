{
    "version": 2,
    "builds": [
        {
            "src": "api/index.php",
            "use": "@vercel/php"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "public/$1"
        }
    ]
}
