{
    "version": 2,
    "builds": [
      {
        "src": "index.php",
        "use": "vercel-php@0.6.0"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.php"
      }
    ],
    "functions": {
      "index.php": {
        "runtime": "vercel-php@0.6.0"
      }
    },
    "env": {
      "NODE_VERSION": "18"
    }
}